{
  "overrides": [
    {
      "files": ["*.jsx", "*.js"]
    }
  ],
  "env": {
    "browser": true,
    "es2021": true
  },
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "extends": [
    "eslint:recommended",
    "next/core-web-vitals",
    "prettier",
    "next",
    "plugin:css-modules/recommended"
  ],
  "plugins": ["unused-imports", "css-modules"]
}
